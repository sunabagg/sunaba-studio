{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Editor",
      "type": "coreclr",
      "request": "launch",
      "program": "${config:godot-dotnet-tools.executablePath}",
      "preLaunchTask": "build-editor",
      "args": [
        "--path",
        "${workspaceRoot}/",
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Attach to Process",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
